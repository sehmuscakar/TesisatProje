@model IEnumerable<DataAccessLayer.Entities.Carousel>
 
<!-- Carousel Start -->
<div class="container-fluid p-0 mb-5">
    <div class="owl-carousel header-carousel position-relative">
           @foreach (var item in Model)
            {
                
        <div class="owl-carousel-item position-relative">
         
           
                <img class="img-fluid" src="~/Img/@item.Image" alt="">
            <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(0, 0, 0, .4);">
                <div class="container">
                    <div class="row justify-content-start">
                        <div class="col-10 col-lg-8">
                            <h5 class="text-white text-uppercase mb-3 animated slideInDown">@item.CityBranch</h5>
                            <h1 class="display-3 text-white animated slideInDown mb-4">@item.City</h1>
                            <p class="fs-5 fw-medium text-white mb-4 pb-2">@item.CityDescription</p>
                                <a href="" class="btn btn-secondary py-md-3 px-md-5 animated slideInRight">
    Ücretsiz Fiyat Teklifi Alın
    </a>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
        }
    </div>
</div>